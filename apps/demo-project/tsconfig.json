{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"composite": true,
		"jsx": "preserve",
		"jsxImportSource": "vue",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
		"resolveJsonModule": true,
		"allowJs": true,
		"strict": true,
		"declaration": true,
		"declarationMap": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": false,
		"verbatimModuleSyntax": true
	},
	"references": [
		{
			"path": "./tsconfig.app.json"
		},
		{
			"path": "./tsconfig.spec.json"
		}
	],
	"files": [],
	"include": [],
	"exclude": []
}
