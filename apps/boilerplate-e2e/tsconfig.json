{
	"module": "commonjs",
	"moduleResolution": "node",
	"preserveValueImports": false,
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"baseUrl": "./",
		"module": "commonjs",
		"types": [
			"node",
			"@types/node",
			"cypress"
		],
		"allowJs": true,
		"strictNullChecks": true,
		"outDir": "../../dist/out-tsc",
		"sourceMap": false
	},
	"include": [
		"src",
		"**/*.cy.ts",
		"**/*.cy.tsx",
		"**/*.cy.js",
		"**/*.cy.jsx",
		"**/*.d.ts",
		"./cypress.config.ts"
	],
	"exclude": [
		"node_modules"
	]
}
