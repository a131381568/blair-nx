{
	"name": "cts-back-end",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/cts-back-end/src",
	"projectType": "application",
	"targets": {
		"start": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "./",
				"command": "node main.js"
			}
		},
		"serve": {
			"executor": "@nx/js:node",
			"defaultConfiguration": "local",
			"options": {
				"buildTarget": "cts-back-end:build",
				"runBuildTargetDependencies": false
			},
			"configurations": {
				"local": {
					"buildTarget": "cts-back-end:build:local"
				},
				"development": {
					"buildTarget": "cts-back-end:build:development"
				},
				"production": {
					"buildTarget": "cts-back-end:build:production"
				}
			},
			"dependsOn": ["copy-env", "build"]
		},
		"build": {
			"dependsOn": ["copy-env"]
		},
		"lint": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/{projectName}",
				"command": "eslint ."
			}
		},
		"lint:fix": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/{projectName}",
				"command": "eslint . --fix"
			}
		},
		"copy-env": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "{workspaceRoot}",
				"command": "node tools/copy-env.ts {projectName}"
			}
		},
		"prisma:m": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/cts-back-end",
				"command": "prisma migrate dev"
			}
		},
		"prisma:g": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/cts-back-end",
				"command": "prisma generate"
			}
		},
		"prisma:pl": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/cts-back-end",
				"command": "prisma db pull"
			}
		},
		"prisma:ps": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/cts-back-end",
				"command": "prisma db push"
			}
		},
		"prisma:val": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/cts-back-end",
				"command": "prisma validate"
			}
		},
		"docker:d": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/cts-back-end/deployment",
				"command": "docker-compose up -d"
			}
		},
		"deploy": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/cts-back-end",
				"command": "pnpm exec nx run-many -t build -p tag:class:cts-build -c production",
				"parallel": false,
				"args": ["--parallel=1"]
			},
			"dependsOn": ["copy-env"]
		}
	},
	"tags": ["type:back_app", "publish:false", "class:cts", "class:cts-build"]
}
